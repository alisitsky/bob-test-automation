        <script>
            function hideArrowInMegaMenu() {
                
                $('.main-navigation .mega-menu-caret').hide();
            }

            
            function hideMegaMenu() {
                
                $('.main-menu-popup').removeClass('appear');
                
                hideArrowInMegaMenu();
            }

            $('#MainMenu2105').on('mouseover', function () {
                
                hideMegaMenu();
                
                let menuWidth = $('#MainMenu2105-menu').width();
                let menuItemWidth = $('#MainMenu2105').width();
                let popupOffsetY = menuWidth / 2 - menuItemWidth / 2;
                let popupOffsetX = this.offsetParent.clientHeight + 9;
                $('#MainMenu2105-menu').css('left', this.offsetLeft - popupOffsetY);
                $('#MainMenu2105-menu').css('top', popupOffsetX);


                $('#MainMenu2105-menu').addClass('appear');
            });

        
        $(document).click(function (event) {
            if (!$(event.target).closest('#MainMenu2105-menu').length && !$(event.target).is('#MainMenu2105-menu')) {
                hideMegaMenu();
            }
            });

        
        
        </script>
    <div id="MainMenu2115-menu" class="main-menu-popup">
