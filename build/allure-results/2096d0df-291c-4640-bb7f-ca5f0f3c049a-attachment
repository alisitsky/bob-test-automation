                let menuItemWidth = $('#MainMenu2112').width();
                let popupOffsetY = menuWidth / 2 - menuItemWidth / 2;
                let popupOffsetX = this.offsetParent.clientHeight + 9;
                $('#MainMenu2112-menu').css('left', this.offsetLeft - popupOffsetY);
                $('#MainMenu2112-menu').css('top', popupOffsetX);


                $('#MainMenu2112-menu').addClass('appear');
            });

        
        $(document).click(function (event) {
            if (!$(event.target).closest('#MainMenu2112-menu').length && !$(event.target).is('#MainMenu2112-menu')) {
                hideMegaMenu();
            }
            });

        
        
        </script>
